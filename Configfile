	serverName: Server2
	listen: 123456789:8080
	root: ./root
	location:
		path: /the2
		autoIndex: true
		root: ./rootchild
		index: index.html index.htm
		errorPage: 399 ./Notworking/index.html
		errorPage: 500 ./Networking/index.html
